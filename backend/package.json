{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node -r dotenv/config dist/app.js",
    "dev": "nodemon --exec babel-node -r ./node_modules/dotenv/config ./src/app.js",
    "test": "NODE_ENV=test nyc mocha -r dotenv/config --require @babel/register \"./src/__tests__/**/*.js\" --timeout 150000 --exit",
    "pretest": "NODE_ENV=test npm run migrate:undo && NODE_ENV=test npm run migrate",
    "coverage": "nyc report --reporter=lcov --reporter=text-summary --reporter=cobertura",
    "migrate": "babel-node ./src/database/scripts/migrate",
    "db:seed": "babel-node ./src/database/scripts/seed",
    "db:seed:undo": "babel-node ./src/database/scripts/seed-rollback",
    "migrate:undo": "babel-node ./src/database/scripts/rollback",
    "db:migrate": "node ./dist/database/scripts/migrate",
    "db:migrate:undo": "node ./dist/database/scripts/rollback",
    "build": "babel src -d dist",
    "lint": "eslint --fix ./",
    "winston": "babel-node ./src/utils/winston.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.14",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "codecov": "^3.8.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "child-process-promise": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash.merge": "^4.6.2",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^4.0.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  }
}
